{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": ".build",
    "rootDir": "./",
    "baseUrl": "."
  },
  "include": ["src/**/*", "__tests__/MeetingController.test.ts"],
  "exclude": ["node_modules"]
}
